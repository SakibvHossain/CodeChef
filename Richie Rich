import java.util.Arrays;
import java.util.Scanner;
public class Lucky {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int t;
		t = scan.nextInt();
		while(t-->0) {
			int A,B,X;
			A=scan.nextInt();
			B=scan.nextInt();
			X=scan.nextInt();
			System.out.println((B-A)/X);
		}
	}
}
